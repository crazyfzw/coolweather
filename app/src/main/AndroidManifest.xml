<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crazyfzw.coolweather">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
      <activity android:name="com.crazyfzw.coolweather.activity.ChooseAreaActivity"
          android:label="@string/app_name">
          <intent-filter>
              <action android:name="android.intent.action.MAIN"/>
              <category android:name="android.intent.category.LAUNCHER"/>
          </intent-filter>
      </activity>

      <activity android:name="com.crazyfzw.coolweather.activity.WeatherActivity">
      </activity>
        <service android:name="com.crazyfzw.coolweather.service.AutoUpdateService"></service>
        <receiver android:name="com.crazyfzw.coolweather.receiver.AutoUpdateReceiver"></receiver>
    </application>

</manifest>
